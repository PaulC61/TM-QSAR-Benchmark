
{
    "name": "TMU-BenchMarkTM",
    "dockerComposeFile": "docker-compose.yml",
    "service": "tmu-developmentv2",
    "workspaceFolder": "/app",
    "forwardPorts": [],
    "postCreateCommand": "echo 'BenchMark-DevContainer is ready'",
    "remoteUser": "root",
    "customizations": {
        "jetbrains": {
          "plugins" : [
            "com.github.copilot",
            "Pythonid"
          ]
        },
        "vscode": {
          "extensions": [
            "ms-python.python",
            "ms-python.debugpy",
            "donjayamanne.python-extension-pack",
            "donjayamanne.python-environment-manager"
          ]
        }
    }
}

 